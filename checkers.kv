<title>:
    BoxLayout:
        Button:
            text: 'Goto settings'
            on_press: root.manager.current = 'settings'
        Button:
            text: 'Quit'
            on_press: app.stop()

<settings>:
    BoxLayout:
        Button:
            text: 'My settings button'
        Button:
            text: 'Back to menu'
            on_press: root.manager.current = 'menu'

<square>:
    canvas:
        Color:
            rgba: 0, 0, 0, 0
        Line:
            width: 2.
            rectangle: (self.x, self.y, self.width, self.height)
